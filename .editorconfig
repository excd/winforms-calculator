# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 100

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[*.{json,yml,yaml,xml,csproj}]
indent_size = 2

[*.cs]
quote_type = double